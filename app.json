{
  "name": "Multi Process Node.js Example",
  "description": "A barebones Node.js app using Express 4",
  "repository": "https://github.com/heroku/node-js-sample",
  "logo": "https://node-js-sample.herokuapp.com/node.png",
  "keywords": ["node", "express", "static", "twitter", "redis", "tortoise", "rabbitMQ"]
   "env": {
          "NODE_ENV": "production",
          "LOGGER_LEVEL": "info",
          "LOGGER_ENABLED": "false",
          "TWITTER_CONSUMER_KEY": "From your twitter App",
"TWITTER_CONSUMER_SECRET": "From your twitter App",
"TWITTER_ACCESS_TOKEN_KEY": "From your twitter App",
"TWITTER_ACCESS_TOKEN_SECRET": "From your twitter App",
"TWITTER_TRACK:" "nodejs,node.js,risingstack,microservice,micro-service",
"RABBITMQ_URI": ""
    },
"addons": [
    "openredis",
    {
      "plan": "mongolab:shared-single-small",
      "as": "MONGO"
    },
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "9.5"
      }
    }
  ],
  }
